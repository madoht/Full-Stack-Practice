extends layouts/main
include mixin/videoBlock

block content
    .search-header
        h3 Searching For : #{searchingBy}
    .search__videos
        if videos.length == 0
            h5 No Video Found
        each item in videos
            +videoBlock({
                title: item.title,
                views: item.views,
                videoFile: item.videoFile,
                id: item.id
            })